{
    "name": "icicleio/file",
    "description": "Asynchronous filesystem component for Icicle.",
    "keywords": [
        "asynchronous",
        "async",
        "file",
        "filesystem"
    ],
    "homepage": "http://icicle.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@icicle.io"
        }
    ],
    "require": {
        "icicleio/icicle": "^0.8.2",
        "icicleio/stream": "^0.3",
        "icicleio/concurrent": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6"
    },
    "minimum-stability": "dev",
    "suggest": {
        "ext-eio": "Extension providing better performance for asynchronous file access."
    },
    "autoload": {
        "psr-4": {
            "Icicle\\File\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Icicle\\Tests\\File\\": "tests"
        }
    }
}

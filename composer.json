{
    "name": "icicleio/filesystem",
    "description": "Asynchronous filesystem component for Icicle.",
    "keywords": [
        "asynchronous",
        "async",
        "file",
        "filesystem"
    ],
    "homepage": "http://icicle.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@icicle.io"
        }
    ],
    "require": {
        "icicleio/icicle": "^2.0",
        "icicleio/stream": "^2.0",
        "icicleio/concurrent": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "minimum-stability": "dev",
    "suggest": {
        "ext-eio": "Extension providing better performance for asynchronous file access. v1.2.6+ required."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Icicle\\File\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Icicle\\Tests\\File\\": "tests"
        }
    }
}
